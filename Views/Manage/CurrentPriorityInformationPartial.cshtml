@model CarWash.Models.PriorityInformationViewModel

<div class="CurrentPriorityContainer col-lg-offset-1">    
        <div class="informContainer">
            <h2>Информация о приоритетах</h2>
            @if (Model.PriorityCarWasher != null)
            {
                <h3>
                    @*@Html.Action("ConnectCarAndCarClient", new { CarClientGuid = item.CarCarId })*@
                    Приоритетный бокс: @Model.PriotityBox.Description
                    <br />
                    Приоритетный автомойщик: @Model.PriorityCarWasher.Surname @Model.PriorityCarWasher.Name @Model.PriorityCarWasher.Patronymic
                </h3>
            }
            else
            {
                <h3>
                    @*@Html.Action("ConnectCarAndCarClient", new { CarClientGuid = item.CarCarId })*@
                    Приоритетный бокс: Не указан
                    <br />
                    Приоритетный автомойщик: Не указан
                </h3>
            }

            @*@Html.ActionLink("Удалить данный приоритет", "DeleteCar",
                new { _boxId = ViewBag.PriorityBox }, new { @class = "carItem" })*@
        </div>    


